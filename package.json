{
  "name": "@unipasswallet/wagmi-connector",
  "version": "1.0.1",
  "description": "Wagmi connector for the UniPass wallet",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "lay2",
  "module": "dist/wagmi-connector.esm.js",
  "size-limit": [
    {
      "path": "dist/wagmi-connector.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/wagmi-connector.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@unipasswallet/ethereum-provider": "^1.0.1",
    "@unipasswallet/popup-types": "^1.0.1",
    "ethers": "^5.7.2",
    "wagmi": "^0.11.0"
  },
  "peerDependencies": {
    "@unipasswallet/ethereum-provider": "^1.0.1",
    "@unipasswallet/popup-types": "^1.0.1",
    "ethers": "^5.7.2",
    "wagmi": "^0.11.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.2",
    "husky": "^8.0.3",
    "size-limit": "^8.1.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  }
}
